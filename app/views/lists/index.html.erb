<div class="home-banner">
  <h1>Your watch list</h1>
  <%= link_to "Create List", new_list_path %>
</div>
<br><br><br>

<div class="container" style="width: 18rem;">

    <h2><% @lists.each do |list| %></h2>
    <img src="https://images.unsplash.com/photo-1478720568477-152d9b164e26?ixlib=rb-1.2.1&ixid=MnwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8&auto=format&fit=crop&w=1770&q=80" class="card-img-top" alt="...">
    <div class="card-body">
    <h5 class="card-title"><h2><%=link_to list.name, list_path(list)%></h2></h5>
    </div>
    <% end %>
</div>
